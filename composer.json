{
    "name": "robbens/laravel-dashboard-upsales",
    "description": "Upsales",
    "keywords": [
        "robbens",
        "laravel-dashboard-upsales"
    ],
    "homepage": "https://github.com/robbens/laravel-dashboard-upsales",
    "license": "MIT",
    "authors": [
        {
            "name": "Robin Nilsson",
            "email": "yo@robin.se",
            "homepage": "https://robin.se",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "guzzlehttp/guzzle": "^6.5",
        "spatie/laravel-dashboard": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Robbens\\UpsalesTile\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Robbens\\UpsalesTile\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Robbens\\UpsalesTile\\UpsalesTileServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
